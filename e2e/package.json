    "expo-status-bar": "~1.11.0",
    "expo-updates": "~0.24.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.5",
    "react-native-dotenv": "^3.4.9",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-reanimated": "^3.6.1",
    "react-native-safe-area-context": "4.7.4",
    "react-native-screens": "~3.27.0",
    "react-native-web": "~0.19.9",
